{
  "name": "bookmak",
  "version": "0.0.1",
  "description": "A Ecommerce of books with AI personalized bookmarks",
  "repository": "git@github.com:gsbenevides2/bookmak.git",
  "author": "Guilherme da Silva Benevides <gsbenevides2@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "ts-node --transpileOnly src/index.ts",
    "build": "tsc",
    "dev": "nodemon --ignore src/apresentation/views --ignore src/apresentation/public --watch src --ext \"*\" --exec \"npm run start\"",
    "lint": "eslint --ext .ts src",
    "createMigration": "typeorm-ts-node-commonjs migration:generate src/persistence/migrations/migration -d src/persistence/dataSourceConfig.ts",
    "runMigration": "typeorm-ts-node-commonjs migration:run -d src/persistence/dataSourceConfig.ts",
    "revertMigration": "typeorm-ts-node-commonjs migration:revert -d src/persistence/dataSourceConfig.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.11.25",
    "@types/pg": "^8.11.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "*"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.2",
    "bcrypt": "^5.1.1",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-fileupload": "^1.5.0",
    "express-minify-html-2": "^2.0.0",
    "express-session": "^1.18.0",
    "openai": "^4.33.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "short-uuid": "^4.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  }
}

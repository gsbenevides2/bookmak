<div class="mt-3 border p-3">
  <h5>Dados do Cliente:</h5>
  <div class="row">
    <div class="col-7 name">Nome: <%= customer.name %></div>
    <div class="col-5 email">Email: <%= customer.email %></div>
    <div class="col-7 cpf">CPF: <%= utils.cpf.mask(customer.cpf) %></div>
    <div class="col-5 dateOfBirth">
      Data de Nascimento: <%= utils.date.mask(customer.dateOfBirth) %>
    </div>
  </div>
</div>